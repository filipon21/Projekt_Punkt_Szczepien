<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: red;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ib.projekt_punkt_szczepien.StartMenuController">
   <children>
      <AnchorPane prefHeight="70.0" prefWidth="600.0" style="-fx-background-color: white;">
         <children>
            <ImageView fx:id="firstAidKitImageView" fitHeight="79.0" fitWidth="159.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../images/apteczka.jpg" />
               </image>
            </ImageView>
            <Label alignment="CENTER" layoutX="116.0" layoutY="13.0" prefHeight="54.0" prefWidth="369.0" text="Aplikacja punktu szczepień">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="patientBtn" layoutX="182.0" layoutY="180.0" mnemonicParsing="false" onAction="#patientBtnOnAction" prefHeight="40.0" prefWidth="236.0" text="Logowanie pacjenta">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="workerBtn" layoutX="182.0" layoutY="255.0" mnemonicParsing="false" onAction="#workerBtnOnAction" prefHeight="40.0" prefWidth="236.0" text="Logowanie pracownika">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="exitBtn" layoutX="485.0" layoutY="369.0" mnemonicParsing="false" onAction="#exitBtnOnAction" prefHeight="31.0" prefWidth="115.0" text="Wyjdź">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
